{
  "compilerOptions": {
	"jsx": "react",
    "baseUrl": "./src",
    "paths": {
      "app/*": ["app/*"]
    },
    "target": "es5",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src"]
}
